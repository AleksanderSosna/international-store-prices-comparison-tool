{
  "name": "International Store Prices Comparison Tool",
  "version": "2023.11.4",
  "private": false,
  "dependencies": {
    "@types/node": "18.16.0",
    "@types/react": "18.2.38",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "6.0.0",
    "customize-cra": "1.0.0",
    "dotenv": "16.3.1",
    "eslint": "8.51.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier": "5.0.1",
    "prettier": "3.1.0",
    "react": "18.2.0",
    "react-app-rewired": "2.2.1",
    "react-dom": "18.2.0",
    "react-router-dom": "6.20.0",
    "react-scripts": "5.0.1",
    "react-tooltip": "5.24.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "4.9.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "env INLINE_RUNTIME_CHUNK=false react-app-rewired build",
    "prettier:write": "prettier --write .",
    "eslint:check": "eslint . --ext .ts,.js,.json",
    "eslint:fix": "eslint . --ext .ts,.js,.json --fix",
    "lint:types": "tsc",
    "lint:check": "npm run eslint:check && npm run lint:types",
    "lint:fix": "npm run prettier:write && npm run eslint:fix && npm run lint:types"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/chrome": "0.0.253",
    "ts-loader": "9.5.1",
    "webpack": "5.89.0",
    "webpack-cli": "5.1.4"
  }
}
